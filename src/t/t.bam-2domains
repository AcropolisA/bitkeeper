bk clone -Bnone -r1.1 $Q C1 C2
cd S
bk lock -wt > ../LOCK &
bk lock -L
cd ../C1
bk push ../C2 > OUT 2>&1
bk _kill `cat ../LOCK`
bk changes -L ../C2 > LOCAL
test -s LOCAL || {
	echo should not have sent csets without BAM data
	exit 1
}
grep -q "unable to update BAM server bk://localhost:$P/S" OUT || {
	echo wrong error message
	cat OUT
	exit 1
}
echo OK

