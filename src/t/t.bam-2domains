echo ---- Locking conflicts ........................................
echo $N Clone C1 to C2 with part in C1 and rest in S w/ S wlocked ...$NL
cd "$HERE"
rm -rf S C1 C2
tar xf TAR
cd S
bk lock -wt > ../LOCK &
bk lock -L
cd ..
bk clone -Bnone C1 C2 > OUT 2>&1
bk _kill `cat LOCK`
test -d C2 && {
