BitKeeper/etc/SCCS/s.ignore
a/b/SCCS/s.f
a/b/c1/SCCS/s.f
a/b/c/SCCS/s.ChangeSet
a/b/c/d/e/SCCS/s.ChangeSet
gcc/SCCS/s.ChangeSet
gdb/SCCS/s.ChangeSet
EOF
cmpfiles GOT WANT
echo OK

echo $N bk sfiles a .................................................$NL
