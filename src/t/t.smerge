   gca10
   won't change
   won't change
EOF
cp gca V1.1.1.1
bk delta $Q -ylocal gca || exit 1
echo OK
echo $N Check default smerge output .................................$NL
bk smerge 1.2 1.1 1.1.1.1 gca
cat - > out2 <<EOF
